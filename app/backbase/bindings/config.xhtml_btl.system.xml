<?xml version="1.0" encoding="UTF-8"?><tdl xmlns="http://www.backbase.com/2006/tdl" xmlns:xi="http://www.w3.org/2001/XInclude">
	<!-- XHTML -->
	<uses namespace="http://www.w3.org/1999/xhtml" src="www.w3.org.1999.xhtml/xhtml.xml"/>

	<!-- skinSettings for system skin must always be loaded if anything from the btl namespace is found -->
	<uses namespace="http://www.backbase.com/2006/btl" src="www.backbase.com.2006.btl/skinSettings/system/skinSettings.xml"/>

	<!-- Load BTL and XHTML Forms profiles when Forms namespace is used -->
	<uses namespace="http://www.backbase.com/2007/forms" src="www.w3.org.1999.xhtml/formsProfile.xml"/>
	<uses namespace="http://www.backbase.com/2007/forms" src="www.backbase.com.2006.btl/formsProfile.xml"/>

	<!-- Backbase Forms controls -->
	<namespace name="http://www.backbase.com/2007/forms" xml:base="www.backbase.com.2007.forms/">
		<uses element="messages message" src="forms/message.xml"/>
		<uses element="fileInput fileInputParameter" src="forms/fileInput.xml"/>
	</namespace>

	<!-- BTL controls -->
	<namespace name="http://www.backbase.com/2006/btl" xml:base="www.backbase.com.2006.btl/">
		<uses behavior="drag dragBase dragTarget" attribute="dragReceive" src="dragAndDrop/dragAndDrop.xml"/>
		<uses behavior="localData" src="dataBinding/localData.xml"/>
		<uses behavior="remoteData" src="dataBinding/remoteData.xml"/>
		<uses behavior="resize resizeBase" src="resize/resize.xml"/>
		<uses behavior="staticData" src="dataBinding/staticData.xml"/>

		<uses element="accordion accordionItem" src="accordion/system/accordion.xml"/>
		<uses element="applet" src="applet/applet.xml"/>
		<uses element="balloon" src="balloon/system/balloon.xml"/>
		<uses element="box" src="box/system/box.xml"/>
		<uses element="broadcaster" src="broadcaster/broadcaster.xml"/>
		<uses element="button" src="button/system/button.xml"/>
		<uses element="calendar" src="calendar/system/calendar.xml"/>
		<uses element="checkBoxGroup checkBoxGroupOption" src="checkBoxGroup/checkBoxGroup.xml"/>
		<uses element="codeHighlighter" src="codeHighlighter/codeHighlighter.xml"/>
		<uses element="comboBox comboBoxOption" src="comboBox/system/comboBox.xml"/>
		<uses element="dataGrid dataGridColGroup dataGridCol" src="dataGrid/system/dataGrid.xml"/>
		<uses element="dataMenu" src="dataMenu/system/dataMenu.xml"/>
		<uses element="dataSource dataContainer dataSchema dataField dataFormatter dataObserver dataPagedObserver" src="dataBinding/dataBinding.xml"/>
		<uses element="dataTree" src="dataTree/system/dataTree.xml"/>
		<uses element="deck deckItem" src="deck/deck.xml"/>
		<uses element="enhanceHTML enhanceHTMLRuleSet enhanceHTMLRule enhanceHTMLAttribute" src="enhanceHTML/enhanceHTML.xml"/>
		<uses element="fieldEditor" src="fieldEditor/fieldEditor.xml"/>
		<uses element="flash" src="flash/flash.xml"/>
		<uses element="groupBox" src="groupBox/system/groupBox.xml"/>
		<uses element="infoBox" src="infoBox/system/infoBox.xml"/>
		<uses element="itemsSelectControl" behavior="userSelect" interface="UIControl" src="itemsSet/itemsSet.xml"/>
		<uses element="label" src="label/label.xml"/>
		<uses element="listBox listBoxOption" src="listBox/system/listBox.xml"/>
		<uses element="listGrid listGridCol" src="listGrid/system/listGrid.xml"/>
		<uses element="loadingMessage" src="loadingMessage/system/loadingMessage.xml"/>
		<uses element="menuBar menuPopUp menuBarItem menuPopUpItem menuBarSeparator menuPopUpSeparator contextMenu" src="menu/system/menu.xml"/>
		<uses element="modal" src="modal/system/modal.xml"/>
		<uses element="navBox" src="navBox/system/navBox.xml"/>
		<uses element="pagerBar pager pagerButton pagerNumbers pagerJumper pagerSeparator" src="pager/system/pager.xml"/>
		<uses element="panelLayout panelLayoutItem" src="panelLayout/panelLayout.xml"/>
		<uses element="panelSet panel" src="panelSet/system/panelSet.xml"/>
		<uses element="populator" src="populator/system/populator.xml"/>
		<uses element="richTextEditor" src="richTextEditor/richTextEditor.xml"/>
		<uses element="sliderOption" src="slider/sliderBase.xml"/>
		<uses element="slider" src="slider/system/slider.xml"/>
		<uses element="spinner" src="spinner/system/spinner.xml"/>
		<uses element="stretch" src="stretch/stretch.xml"/>
		<uses element="suggestBox" src="suggestBox/system/suggestBox.xml"/>
		<uses element="tabBox tab" src="tabBox/system/tabBox.xml"/>
		<uses element="taskBar taskBarItem taskBarButton" src="taskBar/system/taskBar.xml"/>
		<uses element="toolBar toolBarItem toolBarSeparator" src="toolBar/system/toolBar.xml"/>
		<uses element="toolTip" src="toolTip/system/toolTip.xml"/>
		<uses element="tree treeBranch treeLeaf" src="tree/system/tree.xml"/>
		<uses element="treeGrid treeGridCol" src="treeGrid/system/treeGrid.xml"/>
		<uses element="window" src="window/system/window.xml"/>
		<uses element="windowArea" src="windowArea/windowArea.xml"/>
		<uses element="xhtml xml" src="xhtml/xhtml.xml"/>
	</namespace>
</tdl>