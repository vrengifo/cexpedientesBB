<?xml version="1.0" encoding="UTF-8"?><d:tdl xmlns="http://www.w3.org/1999/xhtml" xmlns:b="http://www.backbase.com/2006/btl"  xmlns:d="http://www.backbase.com/2006/tdl" >

	<d:namespace name="http://www.backbase.com/2006/btl">

		<d:uses element="buttonBase" src="../buttonBase.xml"/>

		<d:element name="button" extends="b:buttonBase">
			

			<d:resource type="text/css"><![CDATA[
				.btl-button {
					cursor: pointer;
				}
			]]></d:resource>

			<d:template type="application/xhtml+xml"><button class="btl-button" type="button"><d:content/></button></d:template>

			<d:property name="focusElement" onget="return this.viewNode"/>
		</d:element>
	</d:namespace>
</d:tdl>